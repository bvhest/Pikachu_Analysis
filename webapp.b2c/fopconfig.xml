<fop xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:noNamespaceSchemaLocation="http://svn.apache.org/viewvc/xmlgraphics/fop/trunk/src/foschema/fop-configuration.xsd?view=co">
  <renderers>
    <renderer mime="application/pdf">
      <fonts>
        <font metrics-url="build/webapp/cmc2/xml/fonts/arialuni.ttf.xml"
          embed-url="build/webapp/cmc2/xml/fonts/ARIALUNI.TTF" kerning="yes">
          <font-triplet name="ArialUnicodeMS" style="normal" weight="normal" />
          <font-triplet name="ArialUnicodeMS" style="normal" weight="bold" />
          <font-triplet name="ArialUnicodeMS" style="italic" weight="normal" />
        </font>
      </fonts>
    </renderer>
  </renderers>
</fop>