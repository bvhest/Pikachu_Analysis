      <rng:attribute name="version">
         <rng:ref name="CDATA"/>
      </rng:attribute>
   </rng:optional>
	<rng:optional xmlns="">
      <rng:attribute name="extension-element-prefixes">
         <rng:ref name="CDATA"/>
      </rng:attribute>
   </rng:optional>
	  <rng:optional xmlns="">
      <rng:attribute name="exclude-result-prefixes">
         <rng:ref name="CDATA"/>
