	<rng:start combine="choice">
<rng:ref name="number"/>
</rng:start>
<rng:define name="number">
<rng:element name="number">
<rng:ref name="number"/>
</rng:element>
</rng:define>
	<rng:start combine="choice">
<rng:ref name="choose"/>
</rng:start>
<rng:define name="choose">
<rng:element name="choose">
<rng:ref name="choose"/>
</rng:element>
</rng:define>
	<rng:start combine="choice">
<rng:ref name="if"/>
</rng:start>
<rng:define name="if">
