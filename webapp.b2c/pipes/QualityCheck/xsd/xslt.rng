<?xml version="1.0" encoding="UTF-8"?>
<rng:grammar xmlns:rng="http://relaxng.org/ns/structure/1.0" ns="http://www.w3.org/1999/XSL/Transform" datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
	<a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
		A Schema for XSLT - Don Box (http://www.develop.com/dbox)
		Thanks to Curt Arnold for catching some errors along the way.
	</a:documentation>
	<!-- internal types -->
	<rng:define name="CDATA">
		<rng:ref name="string"/>
	</rng:define>
	<rng:define name="avt">
		<a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
			
		</a:documentation>
		<rng:ref name="string"/>
	</rng:define>
	<rng:define name="pattern">
		<a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
			
		</a:documentation>
		<rng:ref name="string"/>
	</rng:define>
	<rng:define name="expr">
		<a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
			
		</a:documentation>
		<rng:ref name="string"/>
	</rng:define>
	<rng:define name="single-multiple-any">
		<rng:choice>
			<rng:value>single</rng:value>
			<rng:value>multiple</rng:value>
			<rng:value>any</rng:value>
		</rng:choice>
	</rng:define>
	<rng:define name="XPathNumber">
		<a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">
			
		</a:documentation>
		<rng:ref name="decimal"/>
	</rng:define>
	<rng:define name="char">
		<rng:ref name="string"/>
			<!--rng:param name="minLength">1</rng:param>
			<rng:param name="maxLength">1</rng:param-->
		
	</rng:define>
	<rng:define name="yesno">
		<rng:choice>
			<rng:value>yes</rng:value>
			<rng:value>no</rng:value>
		</rng:choice>
	</rng:define>
	<rng:define name="QNames">
		<rng:list>
<rng:ref name="QName"/>
</rng:list>
	</rng:define>
	<!-- literal result element attributes -->
	<rng:optional>
<rng:attribute name="version">
<rng:ref name="CDATA"/>
</rng:attribute>
</rng:optional>
	<rng:optional>
<rng:attribute name="extension-element-prefixes">
<rng:ref name="CDATA"/>
</rng:attribute>
</rng:optional>
	<rng:optional>
<rng:attribute name="exclude-result-prefixes">
<rng:ref name="CDATA"/>
</rng:attribute>
</rng:optional>
	<rng:optional>
<rng:attribute name="use-attribute-sets">
<rng:ref name="QNames"/>
</rng:attribute>
</rng:optional>
	<!-- exemplars for the equiv classes -->
	<rng:start combine="choice">
<rng:ref name="instruction"/>
</rng:start>
<rng:define name="instruction">
<rng:element name="instruction"/>
</rng:define>
	<rng:start combine="choice">
<rng:ref name="char-instruction"/>
</rng:start>
<rng:define name="char-instruction">
<rng:element name="char-instruction"/>
</rng:define>
	<rng:define name="char-template">
		
			<rng:ref name="char-instruction"/>
		
	</rng:define>
	<rng:define name="template">
		
			<rng:ref name="instruction"/>
			<rng:element>
<rng:anyName/>
<rng:text/>
</rng:element>
		
	</rng:define>
	<rng:define name="variable-definition">
		
			<rng:ref name="template"/>
				<rng:attribute name="name">
<rng:ref name="QName"/>
</rng:attribute>
				<rng:optional>
<rng:attribute name="select">
<rng:ref name="expr"/>
</rng:attribute>
</rng:optional>
			
		
	</rng:define>
	<rng:define name="char-template-with-space">
		
			<rng:ref name="char-template"/>
				<rng:attribute name="space" ns="http://www.w3.org/XML/1998/namespace">
<rng:choice>
<rng:value>default</rng:value>
<rng:value>preserve</rng:value>
</rng:choice>
</rng:attribute>
			
		
	</rng:define>
	<rng:define name="template-with-space">
		
			<rng:ref name="template"/>
				<rng:attribute name="space" ns="http://www.w3.org/XML/1998/namespace">
<rng:choice>
<rng:value>default</rng:value>
<rng:value>preserve</rng:value>
</rng:choice>
</rng:attribute>
			
		
	</rng:define>
	<rng:define name="named-template">
		
			<rng:ref name="template-with-space"/>
				
					<rng:element name="param">
<rng:ref name="variable-definition"/>
</rng:element>
				
				<rng:optional>
<rng:attribute name="match">
<rng:ref name="pattern"/>
</rng:attribute>
</rng:optional>
				<rng:optional>
<rng:attribute name="name">
<rng:ref name="QName"/>
</rng:attribute>
</rng:optional>
				<rng:optional>
<rng:attribute name="priority">
<rng:ref name="XPathNumber"/>
</rng:attribute>
</rng:optional>
				<rng:optional>
<rng:attribute name="mode">
<rng:ref name="QName"/>
</rng:attribute>
</rng:optional>
			
		
	</rng:define>
	<rng:define name="conditional-template">
		
			<rng:ref name="template-with-space"/>
				<rng:optional>
<rng:attribute name="test">
<rng:ref name="expr"/>
</rng:attribute>
</rng:optional>
			
		
	</rng:define>
	<rng:define name="import-or-include">
		<rng:attribute name="href">
<rng:ref name="anyURI"/>
</rng:attribute>
	</rng:define>
	<rng:define name="sort">
		<rng:optional>
<rng:attribute name="select" a0:defaultValue="." xmlns:a0="http://relaxng.org/ns/compatibility/annotations/1.0">
<rng:ref name="expr"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="lang">
<rng:ref name="avt"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="data-type" a1:defaultValue="text" xmlns:a1="http://relaxng.org/ns/compatibility/annotations/1.0">
<rng:ref name="avt"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="order" a2:defaultValue="ascending" xmlns:a2="http://relaxng.org/ns/compatibility/annotations/1.0">
<rng:ref name="avt"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="case-order">
<rng:ref name="avt"/>
</rng:attribute>
</rng:optional>
	</rng:define>
	<rng:define name="for-each">
		
			<rng:ref name="template-with-space"/>
				
					<rng:zeroOrMore>
<rng:element name="sort">
<rng:ref name="sort"/>
</rng:element>
</rng:zeroOrMore>
				
				<rng:attribute name="select">
<rng:ref name="expr"/>
</rng:attribute>
			
		
	</rng:define>
	<rng:define name="choose">
		
			<rng:oneOrMore>
<rng:element name="when">
<rng:ref name="conditional-template"/>
</rng:element>
</rng:oneOrMore>
			<rng:optional>
<rng:element name="otherwise">
<rng:ref name="template-with-space"/>
</rng:element>
</rng:optional>
		
	</rng:define>
	<rng:define name="call-template">
		
			<rng:zeroOrMore>
<rng:element name="with-param">
<rng:ref name="variable-definition"/>
</rng:element>
</rng:zeroOrMore>
		
		<rng:attribute name="name">
<rng:ref name="QName"/>
</rng:attribute>
	</rng:define>
	<rng:define name="apply-templates">
		<rng:zeroOrMore>
<rng:choice>
			<rng:element name="with-param">
<rng:ref name="variable-definition"/>
</rng:element>
			<rng:element name="sort">
<rng:ref name="sort"/>
</rng:element>
		</rng:choice>
</rng:zeroOrMore>
		<rng:optional>
<rng:attribute name="select" a3:defaultValue="node()" xmlns:a3="http://relaxng.org/ns/compatibility/annotations/1.0">
<rng:ref name="expr"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="mode">
<rng:ref name="QName"/>
</rng:attribute>
</rng:optional>
	</rng:define>
	<rng:define name="copy">
		
			<rng:ref name="template-with-space"/>
				<rng:optional>
<rng:attribute name="use-attribute-sets">
<rng:ref name="QNames"/>
</rng:attribute>
</rng:optional>
			
		
	</rng:define>
	<rng:define name="copy-of">
		<rng:attribute name="select">
<rng:ref name="expr"/>
</rng:attribute>
	</rng:define>
	<rng:define name="value-of">
		
			<rng:ref name="copy-of"/>
				<rng:optional>
<rng:attribute name="disable-output-escaping" a4:defaultValue="no" xmlns:a4="http://relaxng.org/ns/compatibility/annotations/1.0">
<rng:ref name="yesno"/>
</rng:attribute>
</rng:optional>
			
		
	</rng:define>
	<rng:define name="message">
		
			<rng:ref name="template-with-space"/>
				<rng:optional>
<rng:attribute name="terminate" a5:defaultValue="no" xmlns:a5="http://relaxng.org/ns/compatibility/annotations/1.0">
<rng:ref name="yesno"/>
</rng:attribute>
</rng:optional>
			
		
	</rng:define>
	<rng:define name="apply-imports"/>
	<rng:define name="number">
		<rng:optional>
<rng:attribute name="level" a6:defaultValue="single" xmlns:a6="http://relaxng.org/ns/compatibility/annotations/1.0">
<rng:ref name="single-multiple-any"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="count">
<rng:ref name="pattern"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="from">
<rng:ref name="pattern"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="value">
<rng:ref name="expr"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="format" a7:defaultValue="1" xmlns:a7="http://relaxng.org/ns/compatibility/annotations/1.0">
<rng:ref name="avt"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="lang">
<rng:ref name="avt"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="letter-value">
<rng:ref name="avt"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="grouping-separator">
<rng:ref name="avt"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="grouping-size">
<rng:ref name="avt"/>
</rng:attribute>
</rng:optional>
	</rng:define>
	<rng:define name="text">
		<rng:optional>
<rng:attribute name="disable-output-escaping" a8:defaultValue="no" xmlns:a8="http://relaxng.org/ns/compatibility/annotations/1.0">
<rng:ref name="yesno"/>
</rng:attribute>
</rng:optional>
	</rng:define>
	<rng:define name="processing-instruction">
		
			<rng:ref name="char-template-with-space"/>
				<rng:attribute name="name">
<rng:ref name="avt"/>
</rng:attribute>
			
		
	</rng:define>
	<rng:define name="attribute">
		
			<rng:ref name="processing-instruction"/>
				<rng:optional>
<rng:attribute name="namespace">
<rng:ref name="avt"/>
</rng:attribute>
</rng:optional>
			
		
	</rng:define>
	<rng:define name="element">
		
			<rng:ref name="template-with-space"/>
				<rng:attribute name="name">
<rng:ref name="avt"/>
</rng:attribute>
				<rng:optional>
<rng:attribute name="namespace">
<rng:ref name="avt"/>
</rng:attribute>
</rng:optional>
				<rng:optional>
<rng:attribute name="use-attribute-sets">
<rng:ref name="QNames"/>
</rng:attribute>
</rng:optional>
			
		
	</rng:define>
	<rng:define name="preserve-or-strip-space">
		<rng:attribute name="elements">
<rng:ref name="QNames"/>
</rng:attribute>
	</rng:define>
	<rng:define name="output">
		<rng:optional>
<rng:attribute name="method">
<rng:ref name="QName"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="version">
<rng:ref name="NMTOKEN"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="encoding">
<rng:ref name="CDATA"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="omit-xml-declaration">
<rng:ref name="yesno"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="standalone">
<rng:ref name="yesno"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="doctype-public">
<rng:ref name="string"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="doctype-system">
<rng:ref name="string"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="cdata-section-elements">
<rng:ref name="QNames"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="indent">
<rng:ref name="yesno"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="media-type">
<rng:ref name="CDATA"/>
</rng:attribute>
</rng:optional>
	</rng:define>
	<rng:define name="key">
		<rng:attribute name="name">
<rng:ref name="QName"/>
</rng:attribute>
		<rng:attribute name="match">
<rng:ref name="pattern"/>
</rng:attribute>
		<rng:attribute name="use">
<rng:ref name="expr"/>
</rng:attribute>
	</rng:define>
	<rng:define name="decimal-format">
		<rng:optional>
<rng:attribute name="name">
<rng:ref name="QName"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="decimal-separator" a9:defaultValue="." xmlns:a9="http://relaxng.org/ns/compatibility/annotations/1.0">
<rng:ref name="char"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="grouping-separator" a10:defaultValue="," xmlns:a10="http://relaxng.org/ns/compatibility/annotations/1.0">
<rng:ref name="char"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="infinity" a11:defaultValue="Infinity" xmlns:a11="http://relaxng.org/ns/compatibility/annotations/1.0">
<rng:ref name="CDATA"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="minus-sign" a12:defaultValue="-" xmlns:a12="http://relaxng.org/ns/compatibility/annotations/1.0">
<rng:ref name="char"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="NaN" a13:defaultValue="NaN" xmlns:a13="http://relaxng.org/ns/compatibility/annotations/1.0">
<rng:ref name="CDATA"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="percent" a14:defaultValue="%" xmlns:a14="http://relaxng.org/ns/compatibility/annotations/1.0">
<rng:ref name="char"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="per-mille" a15:defaultValue="â€°" xmlns:a15="http://relaxng.org/ns/compatibility/annotations/1.0">
<rng:ref name="char"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="zero-digit" a16:defaultValue="0" xmlns:a16="http://relaxng.org/ns/compatibility/annotations/1.0">
<rng:ref name="char"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="digit" a17:defaultValue="#" xmlns:a17="http://relaxng.org/ns/compatibility/annotations/1.0">
<rng:ref name="char"/>
</rng:attribute>
</rng:optional>
		<rng:optional>
<rng:attribute name="pattern-separator" a18:defaultValue=";" xmlns:a18="http://relaxng.org/ns/compatibility/annotations/1.0">
<rng:ref name="char"/>
</rng:attribute>
</rng:optional>
	</rng:define>
	<rng:define name="attribute-set">
		
			<rng:zeroOrMore>
<rng:ref name="attribute"/>
</rng:zeroOrMore>
		
		<rng:attribute name="name">
<rng:ref name="QName"/>
</rng:attribute>
		<rng:optional>
<rng:attribute name="use-attribute-sets">
<rng:ref name="QNames"/>
</rng:attribute>
</rng:optional>
	</rng:define>
	<rng:define name="namespace-alias">
		<rng:attribute name="stylesheet-prefix">
<rng:ref name="CDATA"/>
</rng:attribute>
		<rng:attribute name="result-prefix">
<rng:ref name="CDATA"/>
</rng:attribute>
	</rng:define>
	<rng:start combine="choice">
<rng:ref name="apply-templates"/>
</rng:start>
<rng:define name="apply-templates">
<rng:element name="apply-templates">
