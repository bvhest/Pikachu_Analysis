	<rng:define name="stylesheet">
		
			<rng:zeroOrMore>
<rng:element name="import">
<rng:ref name="import-or-include"/>
</rng:element>
</rng:zeroOrMore>
			<rng:zeroOrMore>
<rng:choice>
				<rng:element name="include">
