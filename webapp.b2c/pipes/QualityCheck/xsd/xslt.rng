<rng:element name="apply-imports">
<rng:ref name="apply-imports"/>
</rng:element>
</rng:define>
	<rng:start combine="choice">
<rng:ref name="for-each"/>
</rng:start>
<rng:define name="for-each">
<rng:element name="for-each">
<rng:ref name="for-each"/>
