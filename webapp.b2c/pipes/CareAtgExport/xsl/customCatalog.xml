  <item-descriptor name="olee-item-lang-translation">
		<table name="ph_document_lang_tr" type="primary" id-column-name="olee_item_lang_trans_id">
		    <property name="id" data-type="string" column-name="olee_item_lang_trans_id" />
		    <property name="head" data-type="string" column-name="head" />
		    <property name="body" data-type="string" column-name="body" />
		</table>
	  
		  
    </item-descriptor>
    
    
  
