<?xml version="1.0" encoding="UTF-8"?>
<ObjectKeysConfig>
  <!--
    This is the tree structure for CCR.
    The top level nodes already exist in CCR.
    The child nodes are sent with each export to create any that do not exist.
  -->
  <TreeConfig>
    <Node code="41" name="Product Features">
      <Node code="RTB_DD_00000" name="Dimensional Drawings"/>
      <Node code="RTB_MD_00000" name="RichText - Marketing Downloads"/>
      <Node code="RTB_3D_00000" name="3D Drawings"/>
    </Node>
    <Node code="102" name="System Logos"/>
    <Node code="105" name="Marketing Concept Logos">
      <Node code="RTB_PM_00000" name="Perfect Match"/>
      <Node code="RTB_LF_00000" name="_Lighting Family"/>
    </Node>
    <Node code="107" name="Global Award Logos"/>
    <Node code="109" name="Photometric Products"/>
  </TreeConfig>
  
  <!--
    The mapping maps an element in a product or family to a node in the tree structure above.
    The save-relation attribute indicates whether the "owner" of the ObjectKey should be saved
    in the result.
    The object xpath is used to find any elements that form an ObjectKey.
    The object-code and object-name xpaths specify how to get the code and name for the ObjectKey.
    The Source key attribute may be used to uniquely identify the Source.
  -->
  <TreeMapping>
    <Map save-relation="false">
      <Source key="Dimension diagram table">
        <XPath type="object">RichTexts/RichText[@type='DimensionDiagramTable']/Item</XPath>
        <XPath type="object-code">@code</XPath>
        <XPath type="object-name">if (@referenceName) then @referenceName else @code</XPath>
      </Source>
      <Target treeNode="RTB_DD_00000"/>
    </Map>
    <Map save-relation="false">
      <Source key="3D Drawings">
        <XPath type="object">RichTexts/RichText[@type='3DDrawing']/Item</XPath>
        <XPath type="object-code">@code</XPath>
        <XPath type="object-name">if (@referenceName) then @referenceName else @code</XPath>
      </Source>
      <Target treeNode="RTB_3D_00000"/>
    </Map>
    <Map save-relation="false">
      <Source key="Gifting">
        <XPath type="object">RichTexts/RichText[@type='Gifting']/Item</XPath>
        <XPath type="object-code">@code</XPath>
        <XPath type="object-name">@referenceName</XPath>
      </Source>
      <Target treeNode="41"/>
    </Map>
    <Map save-relation="false">
      <Source key="Marketing downloads">
        <XPath type="object">RichTexts/RichText[@type='MarketingDownload']/Item</XPath>
        <XPath type="object-code">@code</XPath>
        <XPath type="object-name">if (@referenceName) then @referenceName else @code</XPath>
      </Source>
      <Target treeNode="RB_MD_00000"/>
    </Map>
    <Map save-relation="false">
      <Source key="Photometric products">
        <XPath type="object">RichTexts/RichText[@type='PhotometricProduct']/Item</XPath>
        <XPath type="object-code">@code</XPath>
        <XPath type="object-name">if (@referenceName) then @referenceName else @code</XPath>
      </Source>
      <Target treeNode="109"/>
    </Map>

    <Map save-relation="false">
      <Source key="ProductCluster - concept">
        <XPath type="object">ProductClusters/ProductCluster[@type='concept']</XPath>
        <XPath type="object-code">@code</XPath>
        <XPath type="object-name">@referenceName</XPath>
      </Source>
      <Target treeNode="105"/>
    </Map>
    <Map save-relation="false">
      <Source key="ProductCluster - perfect match">
        <XPath type="object">ProductClusters/ProductCluster[@type='perfectmatch']</XPath>
        <XPath type="object-code">@code</XPath>
        <XPath type="object-name">if (@referenceName) then @referenceName else @code</XPath>
      </Source>
      <Target treeNode="RTB_PM_00000"/>
    </Map>
    <Map save-relation="false">
      <Source key="ProductCluster - range">
        <XPath type="object">ProductClusters/ProductCluster[@type='range']</XPath>
        <XPath type="object-code">@code</XPath>
        <XPath type="object-name">@referenceName</XPath>
      </Source>
      <Target treeNode="105"/>
    </Map>
    <Map save-relation="false">
      <Source key="ProductCluster - family">
        <XPath type="object">ProductClusters/ProductCluster[@type='family']</XPath>
        <XPath type="object-code">@code</XPath>
        <XPath type="object-name">Name</XPath>
      </Source>
      <Target treeNode="RTB_LF_00000"/>
    </Map>

    <Map save-relation="false">
      <Source key="Global award">
        <XPath type="object">Award[@AwardType='global']</XPath>
        <XPath type="object-code">AwardCode</XPath>
        <XPath type="object-name">if (AwardName) then AwardName else AwardDescription</XPath>
      </Source>
      <Target treeNode="107"/>
    </Map>

    <Map save-relation="false">
      <Source key="System logo">
        <XPath type="object">SystemLogo</XPath>
        <XPath type="object-code">SystemLogoCode</XPath>
        <XPath type="object-name">SystemLogoName</XPath>
      </Source>
      <Target treeNode="102"/>
    </Map>
    <!--
    <Map save-relation="false">
      <Source key="Family node">
        <XPath type="object">Node[MarketingStatus]</XPath>
        <XPath type="object-code">@code</XPath>
        <XPath type="object-name">@referenceName</XPath>
      </Source>
      <Target treeNode="RTB_LF_00000"/>
    </Map>
    -->
  </TreeMapping>
</ObjectKeysConfig>